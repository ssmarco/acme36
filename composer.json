{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.6.2",
		"silverstripe/framework": "3.6.2",
		"silverstripe/reports": "3.6.2",
		"silverstripe/siteconfig": "3.6.2",
		"silverstripe-themes/simple": "3.1.*",
		"silverstripe/fulltextsearch": "^2.3",
		"algolia/algoliasearch-client-php": "^1.21"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable",
		"silverstripe/fulltextsearch-localsolr": "dev-master"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.6.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
